Host github.com
    IdentityFile ~/.ssh/jzbruno.pem

Host *
    IdentityFile ~/.ssh/jzbruno.pem
    PasswordAuthentication no
    ConnectTimeout 5
    ServerAliveInterval 5
    ServerAliveCountMax 2
    Compression yes
    PermitLocalCommand yes
    ForwardAgent yes
    AddKeysToAgent yes
    TCPKeepAlive yes
